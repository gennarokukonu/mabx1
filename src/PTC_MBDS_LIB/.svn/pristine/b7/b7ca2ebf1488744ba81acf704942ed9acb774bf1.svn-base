&nbsp; temp: numeric<br />
<br />

</td>
<td style="width: 145px;" height="180" valign="top"> <b>Init-Code:</b><br />
&nbsp; x = 0;</td>
<td style="width: 185px;" height="180" valign="top">
<b>Run-Code:</b><br />
if (R) {<br />
&nbsp;x = round(n);<br />
}<br />
&nbsp;temp = x;<br />
&nbsp;if(u) {<br />
&nbsp;&nbsp;x = round(n);<br />
&nbsp;}<bAny integerr="">
&nbsp;else {<br />
&nbsp;&nbsp;if(temp &gt; 0) {<br />
&nbsp;&nbsp;&nbsp;x = x - 1;<br />
&nbsp;&nbsp;}<br />
&nbsp;}<br />
y = ((temp &gt; 0) || u);
