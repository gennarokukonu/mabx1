&nbsp; R: logic<br />
&nbsp; n: numeric<br />
&nbsp; u: logic<br /><span style="font-weight: bold;">Outputs:</span><br />
&nbsp; y: logic<br /><span style="font-weight: bold;">States:</span><br />
